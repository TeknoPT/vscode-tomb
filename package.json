{
    "name": "tomb-new",
    "displayName": "tomb-new",
    "description": "Tomb compiler syntax",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tomb",
            "aliases": ["Tomb", "tomb"],
            "extensions": [".tomb",".tmb"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tomb",
            "scopeName": "tomb",
            "path": "./syntaxes/tomb.tmLanguage.json"
        }]
    }
}